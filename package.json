{
  "name": "app-teste",
  "private": true,
  "version": "0.0.1",
  "main": "main-process/main.js",
  "author": "BCG",
  "description": "Sistema de Login AppTeste",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "emoji-picker-react": "^4.16.1",
    "firebase": "^12.6.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^33.4.11",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.appteste.app",
    "productName": "AppTeste",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "main-process/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}